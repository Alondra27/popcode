{
    "rules": {
        "indent": [
            2,
            2,
            {"SwitchCase": 1}
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "max-len": [
          2,
          79,
          2,
          {"ignoreUrls": true}
        ],
        "no-irregular-whitespace": [
          2
        ],
        "semi": [
            2,
            "always"
        ],
        "strict": [
            2,
            "never"
        ]
    },
    "env": {
        "browser": true,
        "commonjs": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
    },
    "plugins": [
        "react"
    ]
}
