{
  "name": "popcode",
  "version": "0.9.0",
  "description": "Web-based HTML/CSS/JS editor for beginners",
  "contributors": [
    {
      "name": "Mat Brown",
      "email": "mat.a.brown@gmail.com",
      "url": "https://github.com/outoftime"
    },
    {
      "name": "Alejandro AR",
      "url": "https://github.com/kinduff"
    }
  ],
  "dependencies": {
    "PrettyCSS": "^0.3.10",
    "base64-js": "^1.0.2",
    "bowser": "^1.0.0",
    "brace": "^0.5.1",
    "brfs": "^1.4.1",
    "browserify": "^10.2.6",
    "bulkify": "^1.1.1",
    "classnames": "^2.1.5",
    "css": "^2.2.1",
    "envify": "^3.4.0",
    "es6-promise": "^2.3.0",
    "github-api": "^0.10.7",
    "htmllint": "^0.2.4",
    "i18next-client": "^1.10.2",
    "immutable": "^3.7.5",
    "jshint": "^2.8.0",
    "keymirror": "^0.1.1",
    "localforage": "^1.2.7",
    "lodash": "^3.10.0",
    "moment": "^2.10.6",
    "react": "^0.14.3",
    "react-addons-update": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-immutable-proptypes": "^1.5.0",
    "react-redux": "^4.0.6",
    "reactify": "^1.1.1",
    "redux": "^3.0.5",
    "redux-logger": "^2.3.1",
    "redux-thunk": "^1.0.0",
    "slowparse": "^1.1.4",
    "text-encoding": "^0.5.2"
  },
  "engines": {
    "node": "4.2.x"
  },
  "scripts": {
    "start": "./node_modules/.bin/static ./static -a 0.0.0.0 -p ${PORT:-8080}",
    "pretest": "npm run lint",
    "test": "./node_modules/.bin/jest",
    "build": "./node_modules/.bin/browserify --extension .jsx src/application.js -o static/compiled/bundle.js",
    "devserver": "./node_modules/.bin/budo src/application.js --serve compiled/bundle.js --port 8080 --dir ./static --live --cwd ./static --index static/index.html -- --extension .jsx -o static/compiled/bundle.js",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --plugin react src",
    "postinstall": "npm run build",
    "watchtests": "./node_modules/.bin/jest --watch"
  },
  "browserify": {
    "transform": [
      "reactify",
      "brfs",
      "envify"
    ]
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "./node_modules/immutable",
      "./node_modules/es6-promise",
      "./node_modules/keymirror",
      "./node_modules/lodash"
    ]
  },
  "devDependencies": {
    "budo": "^8.0.3",
    "eslint": "^1.4.1",
    "eslint-plugin-react": "^3.3.2",
    "eslint_d": "^2.3.2",
    "immutable-devtools": "0.0.4",
    "jasmine-pit": "^2.0.2",
    "jest-cli": "^0.8.0",
    "node-static": "^0.7.7"
  }
}
