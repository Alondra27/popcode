{
  "src/components/*.jsx": {
    "type": "component",
    "template": "import React from 'react';\n\nclass {camelcase|capitalize} extends React.Component {open}\n  render() {open}\n  {close}\n{close};\n\nexport default {camelcase|capitalize};"
  },
  "src/store.js": {
    "type": "store"
  },
  "src/actions/*.js": {
    "type": "action",
    "alternate": "src/actions/__tests__/{}-test.js"
  },
  "src/actions/__tests__/*-test.js": {
    "type": "actiontest",
    "alternate": "src/actions/{}.js"
  },
  "src/reducers/*.js": {
    "type": "reducer",
    "alternate": "src/reducers/__tests__/{}-test.js"
  },
  "src/reducers/__tests__/*-test.js": {
    "type": "reducertest",
    "alternate": "src/reducers/{}.js"
  },
  "src/validations/*.js": {"type": "validation"},
  "src/static/*": {"type": "static"},
  "src/css/application.css": {"type": "stylesheet"}
}
